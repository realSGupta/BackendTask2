datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Order {
  id        String   @id @default(uuid())
  type      String
  side      String
  tokenIn   String
  tokenOut  String
  amount    Float
  status    String
  txHash    String?
  error     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
